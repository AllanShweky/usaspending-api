{
  "final_name": "summary_view_naics_codes",
  "matview_sql": [
    "SELECT",
    "  \"transaction_normalized\".\"action_date\",",
    "  \"transaction_normalized\".\"fiscal_year\",",
    "  \"transaction_normalized\".\"type\",",
    "  \"transaction_fpds\".\"pulled_from\",",
    "  \"transaction_fpds\".\"naics\" AS naics_code,",
    "  \"transaction_fpds\".\"naics_description\",",
    "  SUM(\"transaction_normalized\".\"federal_action_obligation\") AS \"federal_action_obligation\",",
    "  COUNT(*) counts",
    "FROM",
    "  \"transaction_normalized\"",
    "INNER JOIN",
    "  \"transaction_fpds\" ON (\"transaction_normalized\".\"id\" = \"transaction_fpds\".\"transaction_id\")",
    "WHERE",
    "  \"transaction_normalized\".action_date >= '2007-10-01'",
    "GROUP BY",
    "  \"transaction_normalized\".\"action_date\",",
    "  \"transaction_normalized\".\"fiscal_year\",",
    "  \"transaction_normalized\".\"type\",",
    "  \"transaction_fpds\".\"pulled_from\",",
    "  \"transaction_fpds\".\"naics\",",
    "  \"transaction_fpds\".\"naics_description\""
  ],
  "indexes": [
    {
      "name": "action_date",
      "columns": [{"name": "\"action_date\"", "order": "DESC NULLS LAST"}]
    }, {
      "name": "type",
      "columns": [
        {"name": "\"action_date\"", "order": "DESC NULLS LAST"},
        {"name": "\"type\""}
      ]
    }, {
      "name": "naics",
      "where": "\"naics_code\" IS NOT NULL",
      "columns": [{"name": "\"naics_code\""}]
    }, {
      "name": "tuned_type_and_idv",
      "where": "\"award_type\" IS NULL AND \"pulled_from\" IS NOT NULL",
      "columns": [{"name": "\"award_type\""}, {"name": "\"pulled_from\""}]
     }
  ]
}

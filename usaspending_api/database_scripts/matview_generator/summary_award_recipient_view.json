{
  "final_name": "summary_award_recipient_view",
  "refresh": true,
  "matview_sql": [
    "SELECT",
    "  MD5(array_to_string(sort(array_agg(awards.id::int)), ' '))::uuid AS pk,",
    "  awards.date_signed,",
    "  transaction_normalized.action_date,",
    "  transaction_normalized.fiscal_year,",
    "  awards.type,",
    "  awards.category,",
    "",
    "  MD5(",
    "    UPPER((",
    "      SELECT CONCAT(duns::text, name::text) FROM public.recipient_normalization_pair(",
    "        legal_entity.recipient_name, legal_entity.recipient_unique_id",
    "      ) AS (name text, duns text)",
    "    ))",
    "  )::uuid AS recipient_hash,",
    "  COUNT(*) counts",
    "FROM",
    "  awards",
    "JOIN",
    "  legal_entity ON awards.recipient_id = legal_entity.legal_entity_id",
    "JOIN",
    "  transaction_normalized ON (awards.latest_transaction_id = transaction_normalized.id)",
    "WHERE",
    "  transaction_normalized.action_date >= '2007-10-01'",
    "GROUP BY",
    "  awards.date_signed,",
    "  transaction_normalized.action_date,",
    "  transaction_normalized.fiscal_year,",
    "  awards.type,",
    "  transaction_fpds.pulled_from,",
    "  awards.category,",
    "  legal_entity.recipient_name,",
    "  legal_entity.recipient_unique_id"
  ],
  "indexes": [
    {
      "name": "unique_pk",
      "unique": true,
      "columns": [{"name": "pk"}]
    }, {
      "name": "date_signed",
      "columns": [{"name": "date_signed", "order": "DESC NULLS LAST"}]
    }, {
      "name": "type",
      "columns": [{"name": "type"}]
    }, {
      "name": "pulled_from",
      "where": "pulled_from IS NOT NULL",
      "columns": [{"name": "pulled_from"}]
    }, {
      "name": "recipient_hash",
      "where": "recipient_hash",
      "columns": [{"name": "recipient_hash"}]
    }
  ]
}

  "matview_sql": [
    "WITH sleepy AS (SELECT pg_sleep(10), '-'::text AS wake)",
    "SELECT",
    "  ''::text AS recipient_hash,",
    "  '------'::text AS duns,",
    "  ''::text AS legal_business_name",
    "UNION",
    "SELECT wake, wake, wake from sleepy"
  ],
  "indexes": [
    {
      "name": "unique_hash",
      "unique": true,
      "columns": [{"name": "recipient_hash"}]
    }, {
      "name": "unique_duns",
      "unique": true,
      "where": "duns IS NOT NULL",
      "columns": [{"name": "duns"}]
    }, {
      "name": "recipient_name",
      "columns": [{"name": "legal_business_name"}]
    }
  ]
}
